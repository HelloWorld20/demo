<!-- 加上{{}}的地方是可能需要手动修改的参数 -->
<?xml version="1.0" encoding="gb2312"?>
<STRINGTOXML>
	<!--读取公共部分数据，基本固定-->
	<FixedNode Type="Reference" MapField="to:邮箱地址|toname:客户姓名|querytime:查询时间|title:邮件标题">
		<DataSource Name="_HEADER" Type = "Fix">
		</DataSource>
	</FixedNode>
	<FetchNode Key="info">
		<!-- ItemID：数据中Item标签的ID。 -->
		<GetItem ItemID="{{TELNUM}}">
			<!-- FieldName：页面中索引中文值 -->
            <StringToField FieldName="{{手机号码}}">
				<!-- Name：页面中索引英文值 -->
                <DataSource Name="{{HB_TELNUM}}" Type="Fix">
                </DataSource>
            </StringToField>
        </GetItem>
		<!-- 按需添加GetItem标签组。。。。 -->
	</FetchNode>
</STRINGTOXML>